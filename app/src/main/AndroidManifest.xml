<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="socialcompany.social" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".LoginScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddVoteScreen"
            android:theme="@style/AppTheme.AddVoteTheme" />
        <activity android:name=".VotesScreen" />
        <activity android:name=".VoteProfileScreen" />
        <activity android:name=".FollowerScreen" >
        </activity>
        <activity android:name=".CatScreen" >
        </activity>
        <activity android:name=".HomeScreen" >
        </activity>
    </application>

</manifest>
